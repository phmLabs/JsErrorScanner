{
  "manifest_version": 2,

  "name": "Console2Var",
  "description": "This extension injects js",
  "version": "1.0",

  "permissions": ["<all_urls>"],
  "content_scripts": [
    {
      "matches": ["http://*/*", "https://*/*"],
      "js": ["myscript.js"],
      "run_at": "document_start"
    }
  ]
}