{
  "name": "phmlabs/jserrorscanner",
  "description": "Checks a list of webpages for js errors",
  "minimum-stability": "dev",
  "prefer-stable": true,
  "require": {
    "php": ">= 5.4.0",
    "symfony/console": "~2.2",
    "symfony/yaml": "~2.2",
    "phmlabs/xunitreport": "~1.0.0",
    "phmlabs/html": "~2.1",
    "phmLabs/httpwebdriveradapter": "dev-master",
    "koalamon/koalamonclient": "dev-master",
    "koalamon/cookiemakerhelper": "dev-master",
    "facebook/webdriver": "1.1.*"
  },
  "autoload": {
    "psr-4": {
      "whm\\JsErrorScanner\\": "src/"
    }
  },
  "bin": [
    "bin/JsErrorScanner.php"
  ]
}
